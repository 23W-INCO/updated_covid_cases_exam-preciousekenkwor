{
  "name": "Python 3",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "3.9"
    }
  },
  "settings": {
    "terminal.integrated.shell.linux": null,
    "python.pythonPath": "/usr/local/bin/python",
    "python.autoComplete.extraPaths": [
      "/app"
    ]
  },
  "extensions": [
    "ms-python.python"
  ],
  "forwardPorts": [
    8000
  ],
  "appPort": [
    8000
  ],
  "remoteUser": "vscode"
}
